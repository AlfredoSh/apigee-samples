<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndpoint name="default">
  <PreFlow name="PreFlow">
    <Request/>
    <Response>
      <Step>
        <Condition>message.content != null</Condition>
        <Name>EV-Parse-XML-Fields</Name>
      </Step>
      <Step>
        <Name>X2J-XML-to-JSON</Name>
      </Step>
      <Step>
        <Condition>message.content != null</Condition>
        <Name>EV-Parse-JSON-Fields</Name>
      </Step>
      <Step>
        <Name>AM-Set-Headers</Name>
      </Step>
    </Response>
  </PreFlow>
  <Flows/>
  <PostFlow name="PostFlow">
    <Request/>
    <Response/>
  </PostFlow>
  <HTTPProxyConnection>
    <BasePath>/v1/samples/extract-variables</BasePath>
  </HTTPProxyConnection>
  <RouteRule name="default">
    <TargetEndpoint>default</TargetEndpoint>
  </RouteRule>
</ProxyEndpoint>
